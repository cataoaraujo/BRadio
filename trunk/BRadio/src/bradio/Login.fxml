<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="500.0" styleClass="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="bradio.LoginController">
